{
  "name": "cell-auto-shop",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "cypress:run": "cypress run --record --key=${CYPRESS_RECORD_KEY} --spec \"cypress/integration/active/**/*\"",
    "ci:cypress-run": "start-server-and-test start http://localhost:8080 cypress:run",
    "lint": "vue-cli-service lint",
    "man:publish": "aws s3 sync ./dist s3://cellular-auto-shop --acl public-read --cache-control \"max-age=86400\""
  },
  "dependencies": {
    "airbnb": "0.0.2",
    "basic-system": "^1.0.2",
    "cli-plugin-eslint": "0.0.1",
    "core-js": "^2.6.5",
    "eslint-config-airbnb": "^18.0.1",
    "save": "^2.4.0",
    "vue": "^2.6.10",
    "vuex": "^3.1.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.12.0",
    "@vue/cli-plugin-eslint": "^3.11.0",
    "@vue/cli-service": "^5.0.8",
    "@vue/eslint-config-airbnb": "^5.0.0",
    "babel-eslint": "^10.0.1",
    "cypress": "^3.4.1",
    "eslint": "^5.16.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "start-server-and-test": "^1.10.6",
    "vue-template-compiler": "^2.6.10"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": "off"
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
